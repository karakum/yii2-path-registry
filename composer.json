{
    "name": "karakum/yii2-path-registry",
    "description": "This extension provides a directory generator for file storage based on the maximum occupancy directory.",
    "type": "yii2-extension",
    "keywords": [
        "yii2",
        "extension",
        "upload",
        "path",
        "registry"
    ],
    "homepage": "https://github.com/karakum/yii2-path-registry",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrey Shertsinger",
            "email": "andrey@shertsinger.ru",
            "role": "Maintainer"
        }
    ],
    "support": {
        "issues": "https://github.com/karakum/yii2-path-registry/issues",
        "source": "https://github.com/karakum/yii2-path-registry"
    },
    "require": {
        "yiisoft/yii2": "*"
    },
    "autoload": {
        "psr-4": {
            "karakum\\PathRegistry\\": ""
        }
    }
}
